<form  id="tableForm" method="post" action="/auth/admin_marks">
  <select name="sec" action="/admin_marks" class="form-select w-25 p-3" aria-label="Default select example">
    {{#each section}}
    <option value="{{this.sec}}">{{this.sec}}</option>
      {{/each}}
  </select>
  <button type="#" class="btn btn-dark">Search</button>
</form>

  
<table class="table table-bordered mt-5">
  <thead class="thead-dark">
    <tr>
      <th scope="col">USN</th>
      <th scope="col"class="text-center">18CS51</th>
      <th scope="col"class="text-center">18CS52</th>
      <th scope="col"class="text-center">18CS53</th>
      <th scope="col"class="text-center">18CS54</th>
      <th scope="col"class="text-center">18CS55</th>
      <th scope="col"class="text-center">18CS56</th>
      <th scope="col" class="text-center">Action</th>
    </tr>
  </thead>
  <tbody>
    {{#each rows}}
    <tr>
      <th scope="row">{{this.usn}}</th>
      <td>{{this.18CS51}}</td>
      <td>{{this.18CS52}}</td>
      <td>{{this.email}}</td>
      <td>{{this.phone}}</td>
      <td>{{this.phone}}</td>
      <td>{{this.phone}}</td>
      <td class="text-end">
        <a href="/viewuser/{{this.id}}" type="button" class="btn btn-light btn-small"><i class="bi bi-eye"></i> View</a>
        <a href="/edituser/{{this.id}}" type="button" class="btn btn-light btn-small"><i class="bi bi-pencil"></i>Edit</a>
        <a href="/{{this.id}}" type="button" class="btn btn-light btn-small"><i class="bi bi-person-x"></i> Delete</a>
      </td>
    </tr>
    {{/each}}

  </tbody>
</table>
